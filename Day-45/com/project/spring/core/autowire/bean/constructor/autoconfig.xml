<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.project.spring.core.autowire.bean.constructor.Address" name="address1">
		<property name="city"  value="Bilaspur" />
		<property name="country"  value="India" />
	</bean>
	
	<bean class="com.project.spring.core.autowire.bean.constructor.Employee" name="emp1" autowire="byType">
		<property name="id"  value="160" />
		<property name="name"  value="Thanendra" />
	</bean>
	
	
</beans>